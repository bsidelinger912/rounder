{
  "name": "rounder",
  "version": "0.0.1",
  "description": "Schedule like a pro",
  "main": "index.js",
  "scripts": {
    "dev": "node_modules/concurrently/src/main.js --kill-others \"yarn watch-server\" \"yarn start-dev\" \"yarn watch-client\"",
    "copy-resources": "cp -a ./src/resources/. ./dist/",
    "crawl": "node src/crawlers",
    "watch-api": "node_modules/concurrently/src/main.js --kill-others \"yarn tsc-api -w\" \"nodemon dist/api/index.js\"",
    "watch-client": "webpack-dev-server --config webpack.config.js",
    "watch-server": "yarn copy-resources && webpack --config webpack.server.config.js -w",
    "build": "webpack --config webpack.config.js -p",
    "build-test": "webpack --config webpack.config.js",
    "build-integrations": "webpack --config webpack.integrations.config.js -p",
    "start-dev": "NODE_ENV=dev nodemon ./src/web/server/index.js",
    "start": "node src/web/server/index.js",
    "start-api": "node dist/api/index.js",
    "scrape": "node src/crawlers/tay",
    "tsc-api": "tsc -p ./tsconfig.api.json"
  },
  "author": "Benny Sidelinger",
  "license": "MIT",
